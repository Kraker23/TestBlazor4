@page "/Contacto"
@inject HttpClient Http
<PageTitle>Contacto</PageTitle>

<h1>🤝🏻 Contacto 🔗</h1>

@if (datos?.ContactaConmigo == null)
{
    <p><em>Loading...</em></p>
    <RadzenProgressBar  ProgressBarStyle="ProgressBarStyle.Success" Value="100" ShowValue="false" Mode="ProgressBarMode.Indeterminate" />
}
else
{

    <div class="container-fluid">
        <div class="row my-5">
            <div class="col-lg-6 col-xl-4 offset-lg-3 offset-xl-4">
                <RadzenCard Class="m-3">
                    <div class="d-flex flex-row p-3">
                        <RadzenImage Path="https://raw.githubusercontent.com/devSouvik/devSouvik/master/gif3.gif" Class="rounded-circle float-start me-3" Style="width: 100px; height: 100px;" onclick="" />

                        <div>
                            <RadzenText TextStyle="TextStyle.Overline" Class="d-flex mt-1 mb-0">Email<b>: @(    data.Email)</b></RadzenText>
                            <RadzenText TextStyle="TextStyle.Body1">
                                <a href="mailto:thekraker23@gmail.com" target="blank" rel="noopener noreferrer"><img src="https://cdn-icons-png.flaticon.com/512/888/888853.png" width="40" height="40" /></a>
                            </RadzenText>
                            <RadzenText TextStyle="TextStyle.Overline" Class="d-flex mt-3 mb-0">Linkedin</RadzenText>
                            <RadzenText TextStyle="TextStyle.Body1">
                                <a href="@data.Linkedin" target="blank" rel="noopener noreferrer"><img src="https://raw.githubusercontent.com/rahuldkjain/github-profile-readme-generator/master/src/images/icons/Social/linked-in-alt.svg" alt="cristianj23" width="40" height="40" /></a>
                            </RadzenText>
                        </div>
                    </div>
                    <RadzenCard Class="my-3 rz-background-color-primary-lighter rz-shadow-0 rz-border-radius-0" style="margin: 0 calc(-1 * var(--rz-card-padding)); padding: 2rem;">
                        <RadzenText TextStyle="TextStyle.H6" Class="rz-color-primary-light"><strong>Información Adicional</strong></RadzenText>
                        <div class="row">
                            <div class="col-md-4">

                                <RadzenText TextStyle="TextStyle.Overline" Class="rz-color-primary-light d-flex mt-3 mb-0">CV en PDF</RadzenText>
                                <RadzenText TextStyle="TextStyle.Body1">
                                   @* <b>@(data.CVenPDF)</b>*@
                                    <a href="@data.CVenPDF" target="blank" rel="noopener noreferrer"><img src="https://upload.wikimedia.org/wikipedia/commons/thumb/8/87/PDF_file_icon.svg/1667px-PDF_file_icon.svg.png" width="40" height="40" /></a>
                                </RadzenText>

                                <RadzenText TextStyle="TextStyle.Overline" Class="rz-color-primary-light d-flex mt-3 mb-0">Github</RadzenText>
                                <RadzenText TextStyle="TextStyle.Body1">
                                  @*  <b>@(data.github)</b>*@
                                    <a href="@data.github" target="blank" rel="noopener noreferrer"><img src="data:image/png;base64," width="40" height="40" /></a>
                                </RadzenText>

                                <RadzenText TextStyle="TextStyle.Overline" Class="rz-color-primary-light d-flex mt-3 mb-0">Github Estudiante</RadzenText>
                                <RadzenText TextStyle="TextStyle.Body1">
                                  @*  <b>@(data.githubEstudiante)</b>*@
                                    <a href="@data.githubEstudiante" target="blank" rel="noopener noreferrer"><img src="data:image/png;base64," width="40" height="40" /></a>
                                </RadzenText>

                                 <RadzenText TextStyle="TextStyle.Overline" Class="rz-color-primary-light d-flex mt-3 mb-0">Codigo de Mi Vida</RadzenText>
                                <RadzenText TextStyle="TextStyle.Body1">
                                 <a href="@data.CodigoMiVida" target="blank" rel="noopener noreferrer"><b>@(data.CodigoMiVida)</b></a>
                                   
                                </RadzenText>
                            </div>
                        </div>
                    </RadzenCard>
                </RadzenCard>

            </div>
        </div>
    </div>


}
@code {
    private datosCV? datos;
    public ContactaConmigo data = new ContactaConmigo();

    protected override async Task OnInitializedAsync()
    {
        datos = await Http.GetFromJsonAsync<datosCV>("sample-data/Datos.json");
        if (datos != null && datos.ContactaConmigo != null)
        {
            data = datos.ContactaConmigo;
        }
    }
}
